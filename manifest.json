{
    "name": "Login",
    "version": "7.6.2",
    "description": "Login Portal",
    "permissions": [
        "activeTab",
        "tabs",
        "storage",
        "contextMenus"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "browser_action": {
        "default_icon": "logo.png"
    },
    "manifest_version": 2,
    "icons": {
        "128": "logo.png"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "contentScript.js"
            ]
        }
    ]
}